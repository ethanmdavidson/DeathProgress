# https://editorconfig.org/
root = true

[*]
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = tab
indent_size = 4
max_line_length = 120

[*.{xml,xsd}]
max_line_length = off
indent_size = 2

[gradlew.bat]
end_of_line = crlf

[*.{yml,yaml}]
indent_size = 2
indent_style = space
